<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.mariotaku.refreshnow.sample">

    <uses-sdk />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BasicUsageActivity"
            android:label="Basic usage" />
        <activity
            android:name=".StyledByCodeActivity"
            android:label="Styled (by code)" />
        <activity
            android:name=".StyledByXMLActivity"
            android:label="Styled (by XML)" />
        <activity
            android:name=".PullUpOnlyActivity"
            android:label="Refresh mode (pull up)" />
        <activity
            android:name=".CustomConfigActivity"
            android:label="Custom config" />
    </application>

</manifest>